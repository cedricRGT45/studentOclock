m: 'node',\n\ttarget: 'node12',\n\tinject: ['pathTo/shims.js'],\n\tdefine: {\n\t\tesbuild_app_dir: `\"${config.kit.appDir}\"`\n\t}\n}\n```\n\n## Changelog\n\n[The Changelog for this package is available on GitHub](https://github.com/sveltejs/kit/blob/master/packages/adapter-node/CHANGELOG.md).\n\n## License\n\n[MIT](LICENSE)\n","exports":{"import":"./index.js"},"scripts":{"dev":"rollup -cw","lint":"eslint --ignore-path .gitignore \"**/*.{ts,js,svelte}\" && npm run check-format","test":"c8 uvu tests","build":"rollup -c","format":"npm run check-format -- --write","check-format":"prettier --check . --config ../../.prettierrc --ignore-path .gitignore"},"_npmUser":{"name":"svelte-admin","email":"richard.a.harris+svelte@gmail.com"},"_resolved":"","_integrity":"","_npmVersion":"6.14.14","description":"[Adapter](https://kit.svelte.dev/docs#adapters) for SvelteKit apps that generates a standalone Node server.","directories":{},"_nodeVersion":"12.22.4","dependencies":{"esbuild":"^0.12.5","tiny-glob":"^0.2.9"},"_hasShrinkwrap":false,"readmeFilename":"README.md","devDependencies":{"c8":"^7.7.2","uvu":"^0.5.1","sirv":"^1.0.12","polka":"^1.0.0-next.15","rollup":"^2.55.0","node-fetch":"^3.0.0-beta.9","compression":"^1.7.4","@sveltejs/kit":"1.0.0-next.142","@rollup/plugin-json":"^4.1.0"},"_npmOperationalInternal":{"tmp":"tmp/adapter-node_1.0.0-next.39_1628179223220_0.7287727206937458","host":"s3://npm-registry-packages"}},"1.0.0-next.40":{"name":"@sveltejs/adapter-node","version":"1.0.0-next.40","_id":"@sveltejs/adapter-node@1.0.0-next.40","maintainers":[{"name":"rich_harris","email":"richard.a.harris@gmail.com"},{"name":"svelte-admin","email":"richard.a.harris+svelte@gmail.com"},{"name":"conduitry","email":"npm@chor.date"},{"name":"dominik_g","email":"dominik.goepel@gmx.de"}],"dist":{"shasum":"115d59455cced12f15181e021ad126efd2042115","tarball":"https://registry.npmjs.org/@sveltejs/adapter-node/-/adapter-node-1.0.0-next.40.tgz","fileCount":8,"integrity":"sha512-1FPqp7+PBLhlA29ND25