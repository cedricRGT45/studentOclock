@startuml

class Entreprise {
  - nom: string
  - logo: string
}

class Projet {
  - nom: string
  - description: string
  - technologies: string[]
  - image: string
}

class CarteProjet {
  - projet: Projet
  - pastilleTechno: string
}

class PageProjet {
  - projet: Projet
}

class Contact {
  - email: string
  - téléphone: string
  - adresse: string
}

Entreprise "1" -- "1" Projet
Projet "1" -- "0..*" CarteProjet
Projet "1" -- "1" PageProjet
Entreprise "1" -- "1" Contact

@enduml
